{
  "name": "ui-library",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo",
    "serve-doc": "npm run doc-prepare && ng serve ngx-components-doc",
    "doc-prepare": "npm run docs-typedoc && npm run copy-doc-json && npm run cp-comp-md",
    "docs-typedoc": "rimraf doc && typedoc --options typedoc.json",
    "copy-doc-json": "cp doc/doc.json projects/ngx-components-doc/src/assets/jsons/doc.json",
    "cp-comp-md": "cp projects/applicature/components/README.md projects/ngx-components-doc/src/assets/md/component.md",
    "watch": "npm run watch-styles && npm run watch-components",
    "watch-styles": "ng build @applicature/styles --watch --configuration development",
    "watch-components": "ng build @applicature/components --watch --configuration development",
    "build-doc": "npm run build && npm run doc-prepare && ng build ngx-components-doc",
    "build": "ng build @applicature/styles && ng build @applicature/components",
    "build-packages": "npm run build-styles-package && npm run build-components-package",
    "build-styles-package": "ng build @applicature/styles && cd dist/applicature/styles && npm pack .",
    "build-components-package": "ng build @applicature/components && cd dist/applicature/components && npm pack .",
    "test": "ng test",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.1",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "~13.3.1",
    "@angular/compiler": "~13.3.1",
    "@angular/core": "~13.3.1",
    "@angular/forms": "~13.3.1",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "~13.3.1",
    "@angular/platform-browser-dynamic": "~13.3.1",
    "@angular/router": "~13.3.1",
    "@ethersproject/address": "^5.5.0",
    "@metamask/jazzicon": "^2.0.0",
    "@shapeshiftoss/proto-tx-builder": "^0.2.1",
    "@uiowa/digit-only": "^3.2.0",
    "@uniswap/sdk-core": "^3.0.1",
    "@web3-onboard/core": "2.2.12-alpha.1",
    "@web3-onboard/injected-wallets": "^2.0.8",
    "bignumber.js": "~9.0.2",
    "code-prettify": "^0.1.0",
    "i": "^0.3.7",
    "ngx-markdown": "^13.1.0",
    "npm": "^8.6.0",
    "rxjs": "^7.5.4",
    "tslib": "^2.3.0",
    "url": "^0.11.0",
    "web3": "1.7.0",
    "web3-utils": "1.7.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "~13.3.1",
    "@angular/elements": "^13.3.1",
    "@compodoc/compodoc": "^1.1.18",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.5.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "chromatic": "^6.3.4",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.5.1",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.1",
    "typedoc": "^0.22.13",
    "typescript": "~4.5.2",
    "util": "^0.12.4"
  }
}
